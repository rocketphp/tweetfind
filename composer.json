{
    "name": "rocketphp/tweetfind",
    "description": "Web application that searches tweets by text and location.",
    "type": "application",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Eric Mugerwa",
            "email":  "eric.mugerwa@outlook.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "keywords": [
        "rocketphp",
        "elasticsearch",
        "json",
        "twitter",
        "tweets"
    ],
    "require": { 
        "rocketphp/tweetnest": "dev-master",
        "rocketphp/template": "~1.0",
        "marcj/php-rest-service": "*",
        "bravo3/cache": "~0.1.3"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "phpdocumentor/phpdocumentor": "2.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    }
}